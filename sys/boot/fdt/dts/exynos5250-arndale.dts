/dts-v1/;

/include/ "exynos5250.dtsi"

/ {
	model = "Arndale Board";

	memory {
		device_type = "memory";
		reg = < 0x40000000 0x80000000 >;  /* 2G */
	};

	chosen {
		stdin = &serial2;
		stdout = &serial2;
	};
};
