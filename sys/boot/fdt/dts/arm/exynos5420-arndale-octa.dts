/dts-v1/;

/include/ "exynos5420.dtsi"

/ {
	model = "Arndale Octa Board";

	memory {
		device_type = "memory";
		reg = < 0x20000000 0x40000000 >;  /* 1G */
	};

	SOC: Exynos5@0 {
		serial3: serial@12C30000 {
			status = "okay";
		};
	};

	chosen {
		stdin = &serial3;
		stdout = &serial3;
	};
};
