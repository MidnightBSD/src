/include/ "exynos5.dtsi"

/ {
	compatible = "samsung,exynos5420", "samsung,exynos5";

	SOC: Exynos5@0 {

		serial0: serial@12C00000 {
			clock-frequency = < 50000000 >;
		};

		serial1: serial@12C10000 {
			clock-frequency = < 50000000 >;
		};

		serial2: serial@12C20000 {
			clock-frequency = < 50000000 >;
		};

		serial3: serial@12C30000 {
			clock-frequency = < 50000000 >;
		};
	};
};
