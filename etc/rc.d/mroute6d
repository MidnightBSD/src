#!/bin/sh
#
# $MidnightBSD: src/etc/rc.d/mroute6d,v 1.3 2013/01/09 13:41:05 laffer1 Exp $
#

# PROVIDE: mroute6d
# REQUIRE: netif routing
# BEFORE: NETWORKING
# KEYWORD: nojail

. /etc/rc.subr

name="mroute6d"
rcvar="mroute6d_enable"
command="/usr/sbin/pim6dd"

load_rc_config $name
run_rc_command "$1"
