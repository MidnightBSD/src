#!/bin/sh
#
# $FreeBSD: release/10.0.0/etc/rc.d/mroute6d 230099 2012-01-14 02:18:41Z dougb $
#

# PROVIDE: mroute6d
# REQUIRE: netif routing
# BEFORE: NETWORKING
# KEYWORD: nojail

. /etc/rc.subr

name="mroute6d"
rcvar="mroute6d_enable"
command="/usr/local/sbin/pim6dd"

load_rc_config $name
run_rc_command "$1"
