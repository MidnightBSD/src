$TTL	3600

@	IN	SOA	@host@. root.@host@.  (
				@date@	; Serial
				3600	; Refresh
				900	; Retry
				3600000	; Expire
				3600 )	; Minimum
	IN	NS	@host@.
	IN	PTR	localhost.@domain@.
