   <SC>;<CI>;<ST>;<OC>;<PM>;<AC>;<IO>;<G%>;<IE>;<DS>;\
   <II>;<YI>;<KJ>;<-->;<V%>;<J=>;<Sc>;<=">;<Y=>;<%">;\
   <JE>;<JU>;<JA>;<j=>;<sc>;<='>;<y=>;<%'>;<je>;<ju>;\
   <ja>;<N0>;<io>;<g%>;<ie>;<ds>;<ii>;<yi>;<kj>;<SE>;\
   <v%>
