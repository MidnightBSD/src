   <i=>;<j%>;<k=>;<l=>;<lj>;<m=>;<n=>;<nj>;<o=>;\
   <p=>;<r=>;<s=>;<t=>;<ts>;<u=>;<f=>;<h=>;<c=>;\
   <c%>;<dz>;<s%>;\
