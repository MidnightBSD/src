# $FreeBSD: stable/9/tools/regression/bin/sh/expansion/cmdsubst13.0 233116 2012-03-18 13:02:56Z jilles $

x=1 y=2
[ "$(
	case $((x+=1)) in
	($((y+=1)))	echo bad1 ;;
	($((y-1)))	echo $x.$y ;;
	($((y=2)))	echo bad2 ;;
	(*)		echo bad3 ;;
	esac
)" = "2.3" ] || echo "Error at $LINENO"
[ "$x.$y" = "1.2" ] || echo "Error at $LINENO"
