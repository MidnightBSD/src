# $FreeBSD: src/gnu/usr.bin/Makefile,v 1.84 2005/03/21 05:30:58 kientzle Exp $

.include <bsd.own.mk>

SUBDIR= ${_binutils} \
	${_cc} \
	${_cvs} \
	dialog \
	diff \
	diff3 \
	${_gdb} \
	${_gperf} \
	grep \
	${_groff} \
	${_man} \
	patch \
	${_rcs} \
	sort \
	${_texinfo}
 
.if ${MK_CXX} != "no"
_gperf=		gperf
.if ${MK_GROFF} != "no"
_groff=		groff
.endif
.endif

.if ${MK_CVS} != "no"
_cvs=		cvs
.endif

.if ${MK_MAN} != "no"
_man=		man
.endif

.if ${MK_INFO} != "no"
_texinfo=	texinfo
.endif

.if ${MK_TOOLCHAIN} != "no"
_binutils=	binutils
_cc=		cc
.if ${MK_GDB} != "no"
_gdb=		gdb
.endif
.endif
.if ${MK_RCS} != "no"
_rcs=		rcs
.endif

.include <bsd.subdir.mk>
