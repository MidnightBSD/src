srcdir="@srcdir@"
base=`dirname "$0"`

kdc="${base}/../../kdc/kdc"

list=`${kdc} --builtin-hdb | sed 's/^builtin hdb backends: //'`
oldIFS="$IFS"
IPS=,
set - ${list}
IFS="$oldIFS"

while [ $# != 0 ] ; do
    case $1 in
    db:*) exit 0 ;;
    ndbm:*) exit 0 ;;
    gdbm:*) exit 0 ;;
    db4:*) exit 0 ;;
    db3:*) exit 0 ;;
    ldb:*) exit 0 ;;
    esac
    shift
done

exit 1