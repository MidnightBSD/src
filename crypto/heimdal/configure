main ()
{

		void *schedule = 0;
		MD4_CTX md4;
		MD5_CTX md5;
		SHA_CTX sha1;
		SHA256_CTX sha256;

		MD4_Init(&md4);
		MD5_Init(&md5);
		SHA1_Init(&sha1);
		SHA256_Init(&sha256);
		EVP_CIPHER_iv_length(((EVP_CIPHER*)0));
		#ifdef HAVE_OPENSSL
		RAND_status();
		UI_UTIL_read_pw_string(0,0,0,0);
		#endif

		OpenSSL_add_all_algorithms();
		AES_encrypt(0,0,0);
		DES_cbc_encrypt(0, 0, 0, schedule, 0, 0);
		RC4(0, 0, 0, 0);
  ;
  return 0;
}
