srcdir	= @srcdir@
VPATH	= @srcdir@
SHELL	= /bin/sh

@SET_MAKE@


SUBDIRS	= libedit libukem src

all:	ftp

ftp:	@LIBEDIT@ @LIBUKEM@
	( cd src; ${MAKE} )

libedit.a:
	( cd libedit; ${MAKE} )

libukem.a:
	( cd libukem; ${MAKE} )

install clean:
	@for i in ${SUBDIRS}; do \
	    ( echo "$@ ===> $$i" ; cd $$i ; ${MAKE} $@ ); \
	done

distclean: clean
	@for i in ${SUBDIRS}; do \
	    ( echo "$@ ===> $$i" ; cd $$i ; ${MAKE} $@ ); \
	done
	rm -f Makefile config.cache config.log config.status config.h
